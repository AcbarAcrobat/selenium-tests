<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="Wikia test suite" configfailurepolicy="continue">
    <listeners>
        <listener class-name="com.wikia.webdriver.common.testnglisteners.InvokeMethodAdapter"/>
        <listener class-name="com.wikia.webdriver.common.testnglisteners.GroupTagsMethodInterceptor"/>
    </listeners>
    <test name="testing" preserve-order="true">
        <packages>
            <package name="com.wikia.webdriver.testcases.*" />
        </packages>
    </test>
</suite>
